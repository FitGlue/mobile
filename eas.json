{
    "cli": {
        "version": ">= 5.0.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "simulator": true
            },
            "env": {
                "EXPO_PUBLIC_ENVIRONMENT": "development"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_ENVIRONMENT": "test"
            }
        },
        "production": {
            "distribution": "store",
            "env": {
                "EXPO_PUBLIC_ENVIRONMENT": "production"
            }
        },
        "production-apk": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_ENVIRONMENT": "production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}